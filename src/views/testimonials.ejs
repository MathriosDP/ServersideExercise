<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include('imports'); -%>
  </head>
  <body>
    <%- include('navbar'); -%>
    <div class="container">
        <div class="row">
            <table class="table table-striped table-dark">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Nome</th>
                        <th scope="col">Profiss√£o</th>
                        <th scope="col">Empresa</th>
                        <th scope="col">Testemunho</th>
                    </tr>
                </thead>
                <tbody>
                    <% testimonials.forEach((item) => { %>
                        <tr>
                        <th scope="row">1</th>
                        <td><%= item.name %></td>
                        <td><%= item.title %></td>
                        <td><%= item.company %></td>
                        <td><%= item.testimony %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
  </body>
</html>
